<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>


<!-- 전체 이벤트 갯수 조회-->
<!-- @author seong 5/31  -->

<entry key="selectEventListCount">


	SELECT COUNT(*) COUNT
	FROM EVENT


</entry>

<!-- 전체 이벤트 목록 조회  -->
<!-- @author seong 5/31 -->
<entry key="adminSelectEventList">

SELECT *
		  FROM (
			    SELECT ROWNUM RNUM, A.*
		          FROM (
				        SELECT EVEN_NO
                             , EVEN_TITLE
                             , EVEN_CATEGORY
                             , START_DATE
                             , END_DATE
                             , ENROLL_DATE
                             , STATUS
                        FROM EVENT
                        ORDER BY START_DATE ASC
		                ) A
		        )
WHERE RNUM BETWEEN ? AND ?


</entry>

<!-- 관리자단에서 이벤트 등록 -->
<!-- @author seong 5/31 -->
<entry key="adminInsertEvent">

INSERT INTO EVENT
        ( 
        EVEN_NO
        ,EVEN_TITLE
        ,EVEN_CATEGORY
        ,START_DATE
        ,END_DATE
        ,EVEN_IMG
        )VALUES 
        ( SEQ_ENO.NEXTVAL
        , ?
        , ?
        , ?
        , ?
        , ?
        )


</entry>



</properties>



	