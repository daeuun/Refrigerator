<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<entry key="selectListCount">
		SELECT
		       COUNT(*) COUNT
		  FROM BANNER
	</entry>

	<entry key="selectList">
		SELECT * 
		   FROM (
		         SELECT ROWNUM RNUM, A.*  
		           FROM (
		                 SELECT 
		                        BANNER_NO
		                      , BANNER_NAME
		                      , COMPANY_NAME
		                      , BANNER_CATEGORY
		                      , BANNER_STATUS
		                      , ENROLL_DATE
		                      , MODIFY_DATE
		                      , START_DATE
		                      , END_DATE
		                      , BANNER_IMG
		                      , PAGE
		                  FROM BANNER  
		                 ORDER
		                    BY ENROLL_DATE DESC
		                 ) A
		        )
		WHERE RNUM BETWEEN ? AND ?	
	</entry>


</properties>